class Solution {
    public void moveZeroes(int[] nums) {
        int zero=0;
        int index=0;
        int n=nums.length-1;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                nums[index++]=nums[i];
            }

        }
        for(int j=index;j<nums.length;j++)
        {
            nums[index++]=0;
        }
        return ;
    }
}
