class Solution {
    public int distributeCandies(int[] candies) {
        HashSet<Integer> set=new HashSet<>();
        for(int i=0;i<candies.length;i++)
        {
            set.add(candies[i]);
        }
        if(set.size()<(candies.length)/2)
        {
            return set.size();
        }
        else
        {
            return (candies.length/2);
        }
    }
}
